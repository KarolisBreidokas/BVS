@model ICollection<BVS.Data.Models.ATM>
@{
    ViewData["Title"] = "SubscriptionView";
}

<h2>Prenumeratos</h2>

<div class="container">
    <table class="table">
        <thead>
            <tr>
                <th>KAS</th>
                <th>KOKI</th>
                <th>DUOMENYS</th>
                <th>Atšaukti prenumeratą</th>
            </tr>
        </thead>

        <tbody>
            @for (int i = 0; i < 10; i++)
            {
                <tr>
                    <td>KAS @i</td>
                    <td>KOKI @i</td>
                    <td>DUOMENYS @i</td>
                    <td><button class="btn btn-info" type="button">Atšaukti</button></td>

                </tr>
            }

        </tbody>


    </table>

</div>
